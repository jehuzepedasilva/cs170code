   ],
   "source": [
    "def validate_sub(array, sequence):\n",
    "    s = 0\n",
    "    for i in range(len(array)):\n",
    "        if array[i] == sequence[s]:\n",
    "            s += 1\n",
    "    return s == len(sequence)\n",
    "\n",
    "array = [5, 1, 22, 25, 6, -1, 8, 10]\n",
    "sequence = [1, 6, -1, 10]\n",
    "validate_sub(array, sequence)"
   ]
  },
